{
  "name": "admitad-js-api",
  "version": "2026.01.05-alpha",
  "description": "Modern Admitad API client for Node.js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./nest": {
      "types": "./dist/nest/admitad.module.d.ts",
      "import": "./dist/nest/admitad.module.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrostBy/admitad-js-api.git"
  },
  "bugs": {
    "url": "https://github.com/FrostBy/admitad-js-api/issues"
  },
  "homepage": "https://github.com/FrostBy/admitad-js-api#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "admitad",
    "affiliate",
    "api",
    "cpa"
  ],
  "author": "Vladislav Romanovsky (https://github.com/FrostBy)",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.0",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.13.1"
}
